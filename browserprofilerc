# mybrowsers[name]="browser_type config_dir [options...]"
mybrowsers[shopping]="chr $HOME/.browser-settings/chromium/shopping"
mybrowsers[sns]="chr $HOME/.browser-settings/firefox/sns"

# modify_browsers[orig_command]="your_command"
modify_browsers[google-chrome]=google-chrome-stable
modify_browsers[vivaldi]=vivaldi-stable
